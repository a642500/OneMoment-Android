<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       android:layout_width="match_parent"
       android:layout_height="match_parent">
    <!-- below Android 4.3 -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <co.yishun.onemoment.app.ui.view.shoot.ShootView
            android:id="@+id/shootView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
        <co.yishun.onemoment.app.ui.view.RoundRectImageView
            android:id="@+id/maskImageView"
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:force_square="true"
            app:round_rate="0"/>
    </FrameLayout>


    <include layout="@layout/layout_record_btn"/>
</merge>